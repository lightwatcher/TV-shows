<h1>New Show</h1>

<form action="/create_show">
  <div>
    <label for="title">Title</label><br />
    <input id="title" name="title" type="text" />
  </div>
  <div>
    <label for="descrip">Description</label><br />
    <input id="descrip" name="descrip" type="text" />
  </div>
  <div>
    <label for="episode_num">Episode number</label><br />
    <input id="episode_num" name="episode_num" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>

